	url = {http://www.sciencedirect.com/science/article/pii/S0168170217306299},
	doi = {10.1016/j.virusres.2017.11.008},
	abstract = {RNA viruses that infect microbes are now recognized as an active, persistent and important component of the aquatic microbial community. While some information about the diversity and dynamics of the RNA virioplankton has been derived from culture-based and single gene approaches, research based on viromic and metatransciptomic methods has generated unprecedented insight into this relatively understudied class of microbes. Here, the relevant literature is summarized and discussed, including viromic studies of extracellular aquatic RNA viral assemblages, and transcriptomic studies of active and associated RNA viruses from aquatic environments followed by commentary on the present challenges and future directions of this field of research.},
	language = {en},
	urldate = {2019-12-06},
	journal = {Virus Research},
	author = {Culley, Alexander},
	month = jan,
	year = {2018},
	keywords = {Aquatic microbiology, Metatranscriptomics, Picornavirales, RNA virus, Viral ecology, Viromics},
	pages = {84--89},
	file = {ScienceDirect Full Text PDF:/home/stoughj/Zotero/storage/7HQBAUZZ/Culley - 2018 - New insight into the RNA aquatic virosphere via vi.pdf:application/pdf;ScienceDirect Snapshot:/home/stoughj/Zotero/storage/NWVF2D3C/S0168170217306299.html:text/html}
}

