	month = may,
	year = {2012},
	pmid = {22506599},
	pmcid = {PMC3342519},
	keywords = {Metagenomics, Algorithms, Bacteria, Genome, Bacterial, Sequence Analysis, DNA, Single-Cell Analysis},
	pages = {455--477},
	file = {Full Text:/home/stoughj/Zotero/storage/BVJEP5LX/Bankevich et al. - 2012 - SPAdes a new genome assembly algorithm and its ap.pdf:application/pdf}
